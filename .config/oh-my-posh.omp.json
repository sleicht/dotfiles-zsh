{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": ") ",
    "foreground": "magenta",
    "background": "transparent"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} \ue0b1",
          "foreground": "#26C6DA",
          "background": "#546E7A",
          "type": "os"
        },
        {
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "foreground": "#f8f8f2",
          "background": "#6272a4",
          "type": "session"
        },
        {
          "style": "plain",
          "template": " \uf0e7 ",
          "foreground": "#26C6DA",
          "background": "#546E7A",
          "type": "root"
        },
        {
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#bd93f9",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ abbrevboth 12 22 .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#ffb86c",
          "type": "git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ]
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#8be9fd",
          "type": "node"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "style": "powerline",
          "template": " \ue738 {{ .Full }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#4063D8",
          "type": "java"
        },
        {
          "style": "powerline",
          "template": " <b>K</b> {{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#906cff",
          "type": "kotlin"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#FFDE57",
          "type": "python",
          "invert_powerline": true
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#AE1401",
          "type": "ruby",
          "invert_powerline": true
        },
        {
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": " \ue7b2 {{abbrevboth 10 15 .Project}}{{ if .Region }}@{{ .Region }}{{ end }} \ue7ad  ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#47888d",
          "type": "gcp"
        },
        {
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": "{{ if .Version }} {{ .Version }}{{ end }}",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#ebcc34",
          "type": "terraform"
        },
        {
          "style": "powerline",
          "template": " {{ .FormattedMs }} \uf252 ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#c4a000",
          "type": "executiontime",
          "invert_powerline": true
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "foreground": "#f8f8f2",
          "background": "#ff79c6",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "‚ùØ ",
          "foreground": "#d3d7cf",
          "powerline_symbol": "\ue0b2",
          "background": "#000000",
          "type": "status",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ],
          "invert_powerline": true
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
