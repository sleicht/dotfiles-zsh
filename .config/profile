export PATH="$HOME/.bin:$HOME/.local/bin:$PATH"
if [ -f "$HOME/.bin/san-proxy.sh" ]; then
    . "$HOME/.bin/san-proxy.sh"
fi

if [ -f "$HOME/.local/bin/env" ]; then
    . "$HOME/.local/bin/env"
fi

